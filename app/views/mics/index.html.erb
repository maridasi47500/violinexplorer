<p id="notice"><%= notice %></p>

<h1>Mics</h1>

<table>
  <thead>
    <tr>
      <th>Mic</th>
      <th>Line in</th>
      <th>Gain</th>
      <th>Eq high</th>
      <th>Eq mid</th>
      <th>Eq low</th>
      <th>Low cut</th>
      <th>Fx</th>
      <th>Ban</th>
      <th>Level</th>
      <th>Mixer</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mics.each do |mic| %>
      <tr>
        <td><%= mic.mic %></td>
        <td><%= mic.line_in %></td>
        <td><%= mic.gain %></td>
        <td><%= mic.eq_high %></td>
        <td><%= mic.eq_mid %></td>
        <td><%= mic.eq_low %></td>
        <td><%= mic.low_cut %></td>
        <td><%= mic.fx %></td>
        <td><%= mic.ban %></td>
        <td><%= mic.level %></td>
        <td><%= mic.mixer_id %></td>
        <td><%= mic.type %></td>
        <td><%= link_to 'Show', mic %></td>
        <td><%= link_to 'Edit', edit_mic_path(mic) %></td>
        <td><%= link_to 'Destroy', mic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mic', new_mic_path %>
